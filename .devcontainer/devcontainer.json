{
	"name": "Ziglang devcontainer",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"ZIG_VERSION": "0.14.1",
			"ZLS_VERSION": "0.14.0",
			"MINISIGN_VERSION": "0.12"
		}
	},
	"capAdd": [
		"SYS_PTRACE"
	],
	"securityOpt": [
		"seccomp=unconfined"
	],
	"features": {},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-azuretools.vscode-docker",
				"ziglang.vscode-zig",
				"ianic.zig-language-extras",
				"lorenzopirro.zig-snippets",
				"vadimcn.vscode-lldb",
				"Dart-Code.dart-code"
			]
		}
	},
	"remoteUser": "developer"
}
